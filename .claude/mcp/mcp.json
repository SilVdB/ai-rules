{
  "mcpServers": {
    "linear": {
      "type": "sse",
      "url": "https://mcp.linear.app/sse"
    },
    "context7": {
      "type": "sse",
      "url": "https://mcp.context7.com/sse"
    },
    "deepwiki": {
      "type": "sse",
      "url": "https://mcp.deepwiki.com/sse"
    },
    "sentry": {
      "type": "http",
      "url": "https://mcp.sentry.dev/mcp"
    },
    "playwright": {
      "type": "stdio",
      "command": "bun",
      "args": ["x", "-y", "@playwright/mcp@latest"],
      "env": {}
    },
    "grep": {
      "type": "http",
      "url": "https://mcp.grep.app"
    },
    "OpenZeppelinSolidityContracts": {
      "type": "http",
      "url": "https://mcp.openzeppelin.com/contracts/solidity/mcp"
    },
    "octocode": {
      "type": "stdio",
      "command": "bun",
      "args": ["x", "-y", "octocode-mcp@latest"],
      "env": {}
    },
    "DeepGraph TypeScript MCP": {
      "description": "Comprehensive TypeScript code analysis with type mapping, interface relationships, and module dependency tracking.",
      "command": "bun",
      "args": ["x", "-y", "mcp-code-graph@latest", "microsoft/TypeScript"]
    },
    "DeepGraph React MCP": {
      "description": "Analyze React component hierarchies, state flows, and dependencies. Visualize your React application architecture.",
      "command": "bun",
      "args": ["x", "-y", "mcp-code-graph@latest", "facebook/react"]
    }
  }
}
